{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    // "linebreak-style": ["error", "windows"],
    "linebreak-style": 0,
    "no-underscore-dangle": ["error", { "allow": ["__place"] }]
  }
}
